{
  "name": "ElanSPA",
  "private": true,
  "version": "0.0.1",
  "description": "Elan 2019 PWA",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "start.sw": "npm run start --service-worker",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "^0.12.4",
    "@stencil/router": "^0.2.6",
    "@stencil/sass": "^0.1.0",
    "sgb9-components": "0.0.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "jest": "^22.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serkoma/sgb9-stencil-starter.git"
  },
  "author": "Serkom Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serkoma/sgb9"
  },
  "homepage": "https://github.com/serkoma/sgb9-stencil-starter",
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
